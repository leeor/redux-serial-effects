{
  "name": "redux-serial-effects",
  "version": "0.0.4",
  "description": "Predictable side-effect middleware for the predictable state container",
  "license": "MIT",
  "main": "dist/index.js",
  "config": {
    "eslintopts": "--cache --format codeframe --ext .js,.jsx"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": "eslint $npm_package_config_eslintopts"
    }
  },
  "scripts": {
    "build": "babel src --presets babel-preset-env --ignore '__tests__' --out-dir dist",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint --cache --format codeframe --ext .js,.jsx .",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "release": "wnpm-release --no-shrinkwrap",
    "test": "jest --coverage",
    "watch": "jest --watch",
    "watch:all": "jest --watchAll --coverage"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wix/redux-serial-effects.git"
  },
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^3.16.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-lodash": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "husky": "^0.14.3",
    "jest": "20.0.3",
    "jest-teamcity-reporter": "0.9.0",
    "lint-staged": "^5.0.0",
    "prettier": "^1.7.0",
    "redux": "^3.6.0",
    "wnpm-ci": "*"
  }
}
