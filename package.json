{
  "name": "redux-serial-effects",
  "version": "0.0.4",
  "description": "A middleware library for managing side-effects based on state changes, following an actual vs. expected philosophy, and supporting composability",
  "license": "MIT",
  "main": "dist/index.js",
  "config": {
    "eslintopts": "--cache --format codeframe --ext .js,.jsx"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": "eslint $npm_package_config_eslintopts"
    }
  },
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint --cache --format codeframe --ext .js,.jsx .",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "release": "wnpm-release --no-shrinkwrap",
    "test": "nyc --reporter $([ -z \"$TEAMCITY_VERSION\" ] && echo text || echo teamcity) tape test/*.js | tap-$([ -z \"$test_reporter\" ] && echo spec || echo $test_reporter)",
    "test:watch": "watchman-make -p '**/*.js' --run 'test_reporter=spec npm run test'"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.16.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-lodash": "^2.4.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "nyc": "11.3.0",
    "prettier": "^1.7.0",
    "redux": "^3.6.0",
    "tap-dot": "1.0.5",
    "tap-spec": "4.1.1",
    "tape": "4.8.0",
    "wnpm-ci": "*"
  }
}
